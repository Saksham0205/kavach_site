{
    "builds": [{
        "src": "backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ],
    "env": {
        "MY_ENV_VAR": "value",
        "ANOTHER_ENV_VAR": "another_value"
    }
}
